#include <iostream>
#include <algorithm>
using namespace std;

void anagram() {
    string a, b;
    int arr_a[26] = { 0 }, arr_b[26] = { 0 }, result = 0;
    cin >> a >> b;
    for (char c : a) {
        if (c != ' ') {
            arr_a[int(c - 'a')]++;
        }
    }

    for (char c : b) {
        if (c != ' ') {
            arr_b[int(c - 'a')]++;
        }
    }

    for (int i = 0; i < 26; i++) {
        if (arr_a[i] != arr_b[i]) {
            result++;
        }
    }


    if (result == 0) {
        cout << a << " & " << b << " are " << "anagrams." << endl;
    }
    else
    {
        cout << a << " & " << b << " are NOT " << "anagrams." << endl;
    }
}

int main() {
    int n;
    cin >> n;

    for (int i = 0; i < n; i++) {
        anagram();
    }


    return 0;
}
// 실패 1. 아스키 합 and 소문자 갯수 비교로 풀려 했으나 
//         아스키 1, 5 나 3, 3이나 합은 같기 때문에 무조건 개별 비교 해야함
//         ex) ad, bc
